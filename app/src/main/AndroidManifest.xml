            </intent-filter>
        </activity>

        <service android:name=".NotificationService" />
        <service
            android:name=".Alarm.YourService"
            android:enabled="true" />
        <service
            android:name=".Watsp.AutoStartService"
            android:enabled="true"
            tools:ignore="Instantiatable" />

        <receiver
